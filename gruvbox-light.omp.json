{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#8A0005>\u276f</><#8A0005>\u276f</>",
          "type": "text"
        },
        {
          "foreground": "#635F0C",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#376649",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": " <#7A3661>git:(</>{{ .HEAD }}<#7A3661>)</>",
          "type": "git"
        },
        {
          "foreground": "#A16912",
          "style": "plain",
          "template": " \uf119",
          "type": "status"
        },
        {
          "foreground": "#065563",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
