{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#268BD2>\u276f</><#268BD2>\u276f</>",
          "type": "text"
        },
        {
          "foreground": "#B58900",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#DC322F",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": " <#859900>git:(</>{{ .HEAD }}<#859900>)</>",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " \uf119",
          "type": "status"
        },
        {
          "foreground": "#D33682",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
